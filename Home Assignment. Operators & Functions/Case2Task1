// Implement 2 functions: 
// convertToKelvin(tempCelsius) that converts Celsius to Kelvin 
// convertToKelvin(tempFahrenheit) that converts Fahrenheit to Kelvin

function KelvinToCelcius(tempCelsius){
    let tempKelvin = (tempCelsius + 273.15)
    return tempKelvin;
}
console.log(KelvinToCelcius(0))
//
//new function Fahrenheit to Kelvin
function FahrenheitToKelvin(tempFahrenheit){
    let tempKelvin = (tempFahrenheit - 32) * 5/9 + 273.15
    return tempKelvin;

}
console.log(FahrenheitToKelvin(0));
//
